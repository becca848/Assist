<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <center>
      <AnchorPane prefHeight="233.0" prefWidth="321.0" BorderPane.alignment="CENTER">
         <children>
            <VBox layoutX="-1.0" prefHeight="207.0" prefWidth="293.0" AnchorPane.bottomAnchor="16.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="18.0" AnchorPane.topAnchor="10.0">
               <children>
                  <HBox prefHeight="40.0" prefWidth="240.0">
                     <children>
                        <Label text="Login">
                           <font>
                              <Font size="26.0" />
                           </font>
                        </Label>
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="26.0" prefWidth="340.0" spacing="10.0">
                     <children>
                        <Label text="Username" />
                        <TextField fx:id="username" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="25.0" prefWidth="219.0" spacing="10.0">
                     <children>
                        <Label text="Password " />
                        <PasswordField fx:id="password" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox alignment="CENTER_RIGHT" prefHeight="42.0" prefWidth="273.0">
                     <children>
                        <Button fx:id="Confirm" mnemonicParsing="false" onAction="#Login" text="Confirm" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
